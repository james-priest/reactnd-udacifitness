{
  "expo": {
    "name": "UdaciFitness",
    "description": "This is a hands-on project for Udacity's React Nanodegree program.",
    "githubUrl": "https://github.com/james-priest/reactnd-udacifitness",
    "slug": "reactnd-udacifitness",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/udacifitness-512.png",
